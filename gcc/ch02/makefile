	$(RM) *.o $(ALL) grm088 grm103
 
.SUFFIXES: .o .cpp .c

.c.o:
	$(CC) -c $<
.cpp.o:
	$(CXX) $(CXXFLAGS) -c $<
.c:
	$(CC) -o $@ $<
.cpp:
	$(CXX) $(CXXFLAGS) -o $@ $<
	